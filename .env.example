# Example .env file for Skillvine
# Copy this file to `.env` and fill in real values for local development.

DATABASE_URL=postgres://<DB_USER>:<DB_PASS>@localhost:5432/<DB_NAME>
SECRET_KEY=<GENERATE_A_64_BYTE_HEX>
JWT_SECRET=<GENERATE_RANDOM_SECRET>
AWS_ACCESS_KEY_ID=<YOUR_AWS_KEY>
AWS_SECRET_ACCESS_KEY=<YOUR_AWS_SECRET>
GMAIL_USERNAME=<YOUR_EMAIL>
GMAIL_APP_PASSWORD=<YOUR_GMAIL_APP_PASSWORD>

# Do NOT commit a real `.env` file to version control. Use .env.local for
# machine-local secrets (and ensure it is in .gitignore).